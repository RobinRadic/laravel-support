{
    "name": "laradic/support",
    "description": "Laravel support package",
    "homepage": "https://github.com/laradic/support",
    "keywords": [
        "loop",
        "extensions",
        "blade",
        "laravel"
    ],

    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "radic/tmp-underscore-php": "dev-master"
    },
    "require-dev": {
        "php": ">=5.4.0",
        "laradic/dev": "dev-master"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Support\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
