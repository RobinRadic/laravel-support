{
    "name": "radic/laravel-support",
    "description": "Laravel support package",
    "homepage": "https://github.com/RobinRadic/laravel-support",
    "keywords": [
        "loop",
        "extensions",
        "blade",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:prodeveloper/underscore-php.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "anahkiasen/underscore-php": "dev-master#d59abd7aebdff061c1cdf942622efaccf3113197"
    },
    "require-dev": {
        "php": ">=5.4.0",
        "radic/laravel-dev": "dev-master"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Radic\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radic\\Tests\\Support\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
